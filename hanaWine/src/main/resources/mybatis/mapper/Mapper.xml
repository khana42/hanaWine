<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE	mapper	PUBLIC	"-//mybatis.org//DTD Mapper 3.0//EN"
		"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="com.proj.dao.UserMapper">

	<select id="getUserList" resultType="com.proj.dto.UserVO">
	select uname, uid, upw, email, phone, gender from memberList
	</select>
	
	<select id="getUserByUid" resultType="com.example.demo.dto.UserVO">
	SELECT * FROM memberList
	WHERE num=#{uid}
	</select>
	
</mapper>